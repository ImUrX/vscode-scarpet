{
  "name": "scarpet",
  "displayName": "scarpet",
  "description": "scarpet language extension for vscode",
  "publisher": "ImUrX",
  "author": {
    "name": "ImUrX"
  },
  "license": "MIT",
  "homepage": "https://github.com/ImUrX/vscode-scarpet",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImUrX/vscode-scarpet"
  },
  "bugs": {
    "url": "https://github.com/ImUrX/vscode-scarpet/issues"
  },
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.43.0"
  },
  "keywords": [
    "scarpet",
    "fabric",
    "carpet",
    "fabric-carpet",
    "minecraft"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scarpet",
        "aliases": [
          "scarpet",
          "sc"
        ],
        "extensions": [
          ".sc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scarpet",
        "scopeName": "source.sc",
        "path": "./syntaxes/scarpet.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "scarpet.copyToClipboard",
        "title": "Copy code to clipboard for Minecraft usage",
        "category": "scarpet"
      }
    ],
    "iconThemes": [
      {
        "id": "scarpet",
        "label": "Scarpet",
        "path": "./icon-theme.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:scarpet"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/tokenizr": "^1.5.1",
    "asty-astq": "^1.12.3",
    "tokenizr": "^1.5.7"
  }
}
